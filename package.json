{
  "name": "stock-data-visualizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://chamallakshika09.github.io/stock-data-visualizer",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
